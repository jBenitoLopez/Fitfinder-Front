    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.8",
    "@tsconfig/svelte": "3.0.0",
    "autoprefixer": "10.4.7",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-load-config": "4.0.1",
    "svelte": "3.52.0",
    "svelte-check": "2.9.2",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.1.5",
    "tslib": "2.4.0",
